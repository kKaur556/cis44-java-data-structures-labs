public class Octagon implements Polygon{
    protected double s1;
    public Octagon(double side){
        s1 = side;
    }
    
    @Override
    public double perimeter(){
        return 8 * s1;
    }
    
    @Override
    public double area(){
        return (1+ Math.sqrt(2)) * 2 * Math.pow(s1, 2);
    }
}
