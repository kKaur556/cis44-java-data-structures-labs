class Pentagon implements Polygon{
    protected double s1;
    public Pentagon(double side){
        s1 = side;
    }
    
    @Override
    public double perimeter(){
        return 5 * s1;
    }
    
    @Override
    public double area(){
        return (5 * Math.pow(s1, 2)) / (4 * Math.tan(0.628319));
    }
    
}
