public class Hexagon implements Polygon{
    protected double s1;
    public Hexagon(double side){
        s1 = side;
    }
    
    @Override
    public double perimeter(){
        return 6 * s1;
    }
    
    @Override
    public double area(){
        return ((3*Math.sqrt(3))/2) * Math.pow(s1, 2);
    }
}
